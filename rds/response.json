{
  "type_url": "type.googleapis.com/envoy.api.v2.RouteConfiguration",
  "resources": [
    {
      "@type": "type.googleapis.com/envoy.api.v2.RouteConfiguration",
      "name": "listener_0_for_google",
      "virtual_hosts": [
        {
          "name": "google_host",
          "domains": ["*"],
          "routes": [
            {
              "match": { "prefix": "/" },
              "route": {
                "host_rewrite": "www.google.com",
                "cluster": "service_google"
              }
            }
          ]
        }
      ]
    }
  ]
}
